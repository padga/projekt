{% extends 'base.html.twig' %}


{% block title %}
    {{ 'title.user_edit'|trans({'%id%': user.id|default('')}) }}
{% endblock %}

{% block body %}
    {% include 'default/_form.html.twig' with {
        page_title: 'title.user_edit',
        page_title_param: {'%id%': user.id},
        attr: {
            method: 'PUT',
            action: url('user_edit', {'id': user.id}),
        },
        back_to_list_path: 'dashboard',
    } %}
{% endblock %}