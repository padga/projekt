{% extends 'base.html.twig' %}


{% block title %}
    {{ 'title.user_delete'|trans({'%id%': user.id|default('')}) }}
{% endblock %}

{% block body %}
    {% include 'default/_form.html.twig' with {
        page_title: 'title.user_delete',
        page_title_param: {'%id%': user.id},
